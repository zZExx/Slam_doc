{
    "configurations": [
        {
            "name": "ROS2 Humble",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/install/**/include",  
                "/opt/ros/humble/include/**",             
                "/usr/include/**",                         
                "/usr/include/c++/11/**",                  
                "/usr/include/x86_64-linux-gnu/c++/11/**",
                "/usr/include/eigen3/**"
            ],
            "defines": ["__cplusplus=201703L"],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": ["-std=c++17"],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "/opt/ros/humble/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}