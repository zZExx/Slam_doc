{
    "C_Cpp.default.configurationProvider": "ms-vscode.cpptools",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/install/**",
        "/opt/ros/humble/include/**"
    ],
    "C_Cpp.default.defines": [
        "NAV2_MSGS_FOUND"
    ],
    "files.associations": {
        "*.hpp": "cpp",
        "*.cpp": "cpp"
    }
}
